programa {
  funcao inicio()
  {
    /*
    Sequencia de testes
    */

    converterTemperatura()
    converterTemperatura()
  }

  funcao converterTemperatura()
  {
    real farenheit, celcius

    escreva("Digite a atual temperatura: (C°)\n")
    leia(celcius)

    farenheit = (celcius*1.8)+32

    escreva("A atual temperatura em F° é: ", farenheit, "\n")
  }
}
